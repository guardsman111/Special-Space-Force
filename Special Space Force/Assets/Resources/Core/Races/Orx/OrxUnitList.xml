	<enemyPaths>
		<string>/Races/Orx/Enemies/OrxWarriorEnemy.xml</string>
	</enemyPaths>
	<units>
		<Unit_Class>
			<Unit_Name>Orx Defenders</Unit_Name>
			<Estimated_unit_strength>100</Estimated_unit_strength><!-- Currently 2x number of enemies in the contained enemies collection-->
